<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.websit.mapper.T_freightMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.websit.entity.T_freight">
          <id column="id" property="id" />
        <result column="price" property="price" />
        <result column="prices" property="prices" />
        <result column="freight" property="freight" />
        
    </resultMap>
    <select id="T_freight"  resultType="com.websit.entity.T_freight">
    select   *   from   t_freight 
    </select>
    </mapper>